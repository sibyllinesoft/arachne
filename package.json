{
  "name": "arachnejs-deobfuscator",
  "version": "0.1.0",
  "description": "Advanced JavaScript deobfuscation tool with IR-based analysis and constraint solving",
  "main": "dist/cli/index.js",
  "type": "module",
  "engines": {
    "node": "^22.0.0"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "tsx src/cli/index.ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:mutation": "stryker run",
    "test:e2e": "playwright test",
    "test:differential": "tsx tests/differential/runner.ts",
    "test:contracts": "tsx tests/differential/runner.ts --tags=contract-validation",
    "test:devirt": "tsx tests/differential/runner.ts --tags=devirtualization",
    "test:quickjs": "tsx tests/differential/runner.ts --tags=quickjs",
    "test:v8": "tsx tests/differential/runner.ts --tags=v8 --enable-v8",
    "lint": "eslint src/ tests/ --ext .ts",
    "lint:fix": "eslint src/ tests/ --ext .ts --fix",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "benchmark": "bun run scripts/run-benchmarks.ts",
    "benchmark:report": "bun run scripts/run-benchmarks.ts --format markdown --output benchmark-report.md",
    "benchmark:json": "bun run scripts/run-benchmarks.ts --format json --output benchmark-results.json",
    "bench": "tsx tests/differential/runner.ts --tags=performance",
    "gatekeeper": "python3 scripts/gatekeeper.py",
    "gatekeeper:report": "python3 scripts/gatekeeper.py --output gatekeeper-report.json --verbose",
    "docker:build": "docker build -f infra/Dockerfile -t arachnejs .",
    "docker:dev": "docker-compose -f infra/docker-compose.yml up",
    "smoke": "bash scripts/spinup_smoke.sh",
    "ui:install": "cd ui && npm install",
    "ui:dev": "cd ui && npm run dev",
    "ui:build": "cd ui && npm run build",
    "ui:preview": "cd ui && npm run preview",
    "ui:type-check": "cd ui && npm run type-check"
  },
  "keywords": [
    "deobfuscation",
    "javascript",
    "static-analysis",
    "bytecode",
    "constraint-solving",
    "reverse-engineering"
  ],
  "author": "ArachneJS Team",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^20.10.0",
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "inquirer": "^9.2.12",
    "source-map": "^0.7.4",
    "acorn": "^8.11.2",
    "acorn-walk": "^8.3.0",
    "escodegen": "^2.1.0",
    "estraverse": "^5.3.0",
    "quickjs-emscripten": "^0.23.0",
    "z3-solver": "^4.12.2",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^8.0.0",
    "@stryker-mutator/typescript-checker": "^8.0.0",
    "@stryker-mutator/vitest-runner": "^8.0.0",
    "@types/inquirer": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@playwright/test": "^1.40.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "prettier": "^3.1.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arachnejs/deobfuscator.git"
  },
  "bugs": {
    "url": "https://github.com/arachnejs/deobfuscator/issues"
  },
  "homepage": "https://github.com/arachnejs/deobfuscator#readme"
}